{
    "manifest_version": 2,
  
    "name": "NTHU eat",
    "description": "This extension can help you check whether the restaurant is open",
    "version": "0.0.1",

    "icons": {
      "16": "icon_16.png",
      "48": "icon_128.png",
      "128": "icon_128.png"
    },
    "browser_action": {
      
      "default_icon": "icon_16.png",
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

    
  }