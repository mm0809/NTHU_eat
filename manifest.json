{
    "manifest_version": 2,
  
    "name": "NTHU eat",
    "description": "This extension can help you check whether the restaurant is open",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/",
      "https://*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

    
  }